{
    "name": "hybridmind/topic_counter",
    "type": "phpbb-extension",
    "description": "Count all user topics and display them when viewing a topic",
    "homepage": "https://webocean.info",
    "version": "1.0.1",
    "time": "2020-06-30",
    "keywords": [
        "phpbb",
        "extension",
        "topic_counter"
    ],
    "license": "GPL-2.0",
    "authors": [{
        "name": "hybridmind",
        "homepage": "https://webocean.info",
        "email": "admin@webocean.info",
        "role": "Web Developer"
    }],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Topic counter",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.3.*@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/topic_counter",
            "filename": "version_check"
        }
    }
}